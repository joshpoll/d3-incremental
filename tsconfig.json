{
  "compilerOptions": {
      "target": "es5",
      "sourceMap": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "lib": [
          "es2019",
          "es5",
          "es6",
          "dom"
      ],
      "noUnusedLocals": true,
      "module": "commonjs",
      "strict": false,
      "resolveJsonModule": true,
      "esModuleInterop": true,
  },
  "include": [ "lib/*", "src/*" ]
}
